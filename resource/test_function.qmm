fun first_daffodils_since(n: I32) -> I32 {
    loop {
        var temp = n
        var sum = 0
        var digits = 0
        while temp > 0 {
            temp = temp / 10
            digits = digits + 1
        }
        temp = n
        while temp > 0 {
            var digit = temp % 10
            var power = 1
            var i = 0
            while i < digits {
                power = power * digit
                i = i + 1
            }
            sum = sum + power
            temp = temp / 10
        }
        if sum == n {
            break
        }
        n = n + 1
    }
    n
}


val first = first_daffodils_since(100)
val second = first_daffodils_since(first+1)
val third = first_daffodils_since(second+1)
val forth = first_daffodils_since(third+1)


println(first)
println(second)
println(third)
println(forth)

